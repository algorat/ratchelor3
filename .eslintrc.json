{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "max-len": ["error", {"ignoreComments": true}],
    "no-unused-vars": ["warn"],
    "react/prop-types": "off"
  }
}
